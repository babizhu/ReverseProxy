<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <!-- encoder 默认配置为PatternLayoutEncoder -->
    <encoder>
      <!--<pattern>%d{yyyy/MM/dd-HH:mm:ss} %level [%thread] %caller{1} - %msg%n</pattern>-->
      <pattern>%date %level [%thread] %logger{10} [%line] %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="com.bbz.network.reverseproxy.impl.ClientToProxyConnection" level="debug" additivity="false">
    <!--<appender-ref ref="STDOUT"/>-->

  </logger>

  <logger name="com.bbz.network.reverseproxy" level="debug" additivity="false">
    <!--<appender-ref ref="STDOUT"/>-->

  </logger>

  <root level="debug">
    <!--<appender-ref ref="STDOUT"/>-->

  </root>

</configuration>